+
+all:
+	$(MAKE) -C $(KERNELDIR) M=`pwd` APPARMOR_VER=${REPO_VERSION} $@
+	mv apparmor.ko apparmor-${KERNELVER}.ko
+	mv aamatch/aamatch_pcre.ko aamatch_pcre-${KERNELVER}.ko
