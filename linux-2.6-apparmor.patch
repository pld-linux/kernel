+static int subdomain_inode_mknod(struct inode *inode, struct dentry *dentry,
+				 int mode, dev_t dev)
+{
+	struct subdomain sdcopy, *sd;
+	int error = 0;
+
+	if (!current->security)
+		return 0;
+
+	sd = get_sdcopy(&sdcopy);
+
+	error = sd_perm_dentry(sd, dentry, MAY_WRITE);
+
+	put_sdcopy(sd);
+
+	return error;
