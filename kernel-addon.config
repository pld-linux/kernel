#
# CPU Frequency scaling
#
CONFIG_CPU_FREQ=y
CONFIG_CPU_FREQ_24_API=y
CONFIG_X86_POWERNOW_K6=m
CONFIG_X86_LONGHAUL=m
CONFIG_X86_SPEEDSTEP=m
