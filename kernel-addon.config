CONFIG_DIGEST_SHA384=m
CONFIG_DIGEST_SHA512=m
CONFIG_CRYPTODEV=y
CONFIG_CRYPTOLOOP=m
CONFIG_CRYPTOLOOP_ATOMIC=y
