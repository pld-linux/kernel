+				extr->tdb->tdb_lifetime_bytes_s = extr->tdb->tdb_lifetime_bytes_h;
+			}
+		}
+		if(pfkey_lifetime->sadb_lifetime_addtime &&
+		   (!extr->tdb->tdb_lifetime_addtime_s ||
+		    (pfkey_lifetime->sadb_lifetime_addtime < extr->tdb->tdb_lifetime_addtime_s))) {
