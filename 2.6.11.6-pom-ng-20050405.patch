diff -Nur --exclude '*.orig' linux-2.6.11.6.org/net/ipv4/netfilter/ip_set.c linux-2.6.11.6/net/ipv4/netfilter/ip_set.c
--- linux-2.6.11.6.org/net/ipv4/netfilter/ip_set.c	1970-01-01 01:00:00.000000000 +0100
+++ linux-2.6.11.6/net/ipv4/netfilter/ip_set.c	2005-04-05 10:50:57.000000000 +0200
@@ -0,0 +1,1989 @@
+/* Copyright (C) 2000-2002 Joakim Axelsson <gozem@linux.nu>
+ *                         Patrick Schaaf <bof@bof.de>
+ * Copyright (C) 2003-2004 Jozsef Kadlecsik <kadlec@blackhole.kfki.hu>
